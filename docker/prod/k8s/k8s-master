apiVersion: extensions/v1beta1
kind: Deployment
metadata:
  name: nginx
spec:
  replicas: 1
  strategy: {}
  template:
    spec:
      containers:
      - image: jutonz/k8s-playground-prod-nginx:6
        name: nginx
        ports:
        - containerPort: 80
        resources: {}
      - image: jutonz/k8s-playground-prod-rails:4
        name: rails
        resources: {}
      restartPolicy: Always
status: {}
